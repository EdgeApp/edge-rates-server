{
  "apps": [
    {
      "name": "ratesServer",
      "script": "lib/index.js",
      "instances" : "max",
      "exec_mode" : "cluster",
      "error_file": "logs/server/error.log",
      "out_file": "logs/server/out.log"
    },
    {
      "name": "ratesStorage",
      "script": "lib/indexEngines.js",
      "instances" : 1,
      "error_file": "logs/engines/error.log",
      "out_file": "logs/engines/out.log"
    },
    {
      "name": "ratesEngine",
      "script": "lib/ratesEngine.js",
      "instances" : 1,
      "cron_restart": "*/30 * * * * *",
      "watch": false,
      "autorestart": false,
      "error_file": "logs/engines/error.log",
      "out_file": "logs/engines/out.log"
    },
    {
      "name": "ratesUidEngine",
      "script": "lib/uidEngine.js",
      "instances" : 1,
      "cron_restart": "0 10 * * *",     
      "watch": false,
      "autorestart": false,
      "error_file": "logs/engines/error.log",
      "out_file": "logs/engines/out.log"
    }
  ]
}
